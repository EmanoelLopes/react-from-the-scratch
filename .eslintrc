{
  "settings": {
    "react": {
      "createClass": "createReactClass",
      "pragma": "React",
      "version": "^16.5.0"
    },
    "propWrapperFunctions": ["forbidExtraProps"]
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "react"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb-base"
  ]
}